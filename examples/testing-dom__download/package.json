{
  "name": "download",
  "version": "1.0.0",
  "description": "Download and validate a file",
  "scripts": {
    "cypress:open": "../../node_modules/.bin/cypress open",
    "cypress:run": "../../node_modules/.bin/cypress run",
    "cypress:run:chrome": "../../node_modules/.bin/cypress run --browser chrome",
    "cypress:run:firefox": "../../node_modules/.bin/cypress run --browser firefox",
    "cypress:run:win": "bin-up cypress run",
    "cypress:run:win:record": "bin-up cypress run --record --browser chrome",
    "dev": "../../node_modules/.bin/start-test 8070 cypress:open",
    "start": "../../node_modules/.bin/serve -l 8070 public",
    "start:win": "bin-up serve -l 8070 --no-clipboard",
    "test:ci:chrome": "../../node_modules/.bin/start-test 8070 cypress:run:chrome",
    "test:ci:firefox": "../../node_modules/.bin/start-test 8070 cypress:run:firefox",
    "test:ci:windows": "bin-up start-test start:win http://localhost:8070 cypress:run:win",
    "test:ci:windows:record": "bin-up start-test start:win http://localhost:8070 cypress:run:win:record"
  }
}
